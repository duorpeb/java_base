package day09;

public class CardMain {
	public static void main(String[] args) {
		Card c = new Card();
		
		// 기본 생성자 확인, c.Print();
		c.setShape("#");
		System.out.print("Shape : # \t" + "출력 결과 : ");
		c.Print();
		
		c.setNum("22");
		System.out.print("Num : 22 \t" + "출력 결과 : ");
		c.Print();
		
		c.setShape("◆");
		System.out.print("Shape : ◆ \t" + "출력 결과 : ");
		c.Print();
		
		c.setNum("7");
		System.out.print("Num : 7 \t" + "출력 결과 : ");
		c.Print();
		
		c.setNum("12");
		System.out.print("Num : 12 \t" + "출력 결과 : ");
		c.Print();
		
		c.setShape("◆");
		System.out.print("Shape : ◆ \t" + "출력 결과 : ");
		c.Print();
	}
}
