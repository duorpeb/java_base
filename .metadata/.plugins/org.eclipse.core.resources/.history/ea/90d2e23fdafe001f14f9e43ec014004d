package day09;

public class Exam02SdCourse {
 private
  String course, dateStart, dateEnd;

  // 기본생성자
 	public Exam02SdCourse() {
 	 this.course = course;
 	 this.dateStart = dateStart;
 	 this.dateEnd = dateEnd;
 	}
  
  // 기본 생성자 오버로딩 (1)
  public Exam02SdCourse(String course, String dateStart, String dateEnd) {
	 this.course = course;
	 this.dateStart = dateStart;
	 this.dateEnd = dateEnd;
 }
 

  // Getter(), Setter()
	public String getCourse() {
		return course;
	}

	public void setCourse(String course) {
		this.course = course;
	}

	public String getDateStart() {
		return dateStart;
	}

	public void setDateStart(String dateStart) {
		this.dateStart = dateStart;
	}

	public String getDateEnd() {
		return dateEnd;
	}

	public void setDateEnd(String dateEnd) {
		this.dateEnd = dateEnd;
	}
}
